     <string notr="true">QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox, QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #DEE2E6;
    border-radius: 8px;
    padding: 4px 8px;
    color: #343A40;
}

QLineEdit:focus,
QTextEdit:focus,
QPlainTextEdit:focus,
QSpinBox:focus,
QDoubleSpinBox:focus,
QComboBox:focus {
    border-color: #3A5BA0;
}

/* ComboBox arrow */
QComboBox::drop-down {
    border: none;
    width: 20px;
}
QComboBox::down-arrow {
    image: none;
    width: 0;
    height: 0;
    border-left: 4px solid transparent;
    border-right: 4px solid transparent;
    border-top: 6px solid #6C757D;
}

/* ========= Primary / Secondary Buttons ========= */
/* Set objectName=&quot;primaryButton&quot; or &quot;secondaryButton&quot; */

QPushButton {
    background-color: rgb(193, 39, 45);   /* main red */
    color: #FFFFFF;
    border-radius: 8px;
    border: none;
    padding: 6px 18px;
    font-weight: 600;
}

/* Hover — slightly darker red */
QPushButton:hover {
    background-color: rgb(168, 34, 39);   /* #A82227 */
}

/* Pressed — deeper &amp; stronger red */
QPushButton:pressed {
    background-color: rgb(140, 28, 32);   /* #8C1C20 */
}

/* Disabled buttons */
QPushButton:disabled {
    background-color: #E9ECEF;
    color: #ADB5BD;
}

